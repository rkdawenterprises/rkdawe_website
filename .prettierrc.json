{
    "trailingComma": "none",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "printWidth": 120,
    "arrowParens": "always",
    "bracketSameLine": true,
    "svelteBracketNewLine": false,
    "quoteProps": "consistent",
    "svelteStrictMode": true,
    "svelteAllowShorthand": false,
    "htmlWhitespaceSensitivity": "ignore"
}